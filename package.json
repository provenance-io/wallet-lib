{
  "name": "@provenance/frontlib-wallet",
  "version": "0.1.12",
  "description": "Provenance wallet interface library",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "publishConfig": {
    "registry": "https://nexus.figure.com/repository/figure-npm/"
  },
  "sideEffects": false,
  "files": [
    "lib",
    "esm"
  ],
  "scripts": {
    "clean": "rm -rf ./lib ./esm",
    "clean:docs": "rm -rf ./docs",
    "build": "npm run clean && npm run build-protos && npm run schema-to-ts && npm run build:commonjs && npm run build:esm && npm run tsc:esm && npm run tsc:commonjs",
    "build:esm": "BABEL_ENV=esm babel src -d esm --extensions \".ts,.js,.tsx\" --copy-files --ignore \"src/**/*.stories.tsx,src/proto/*,src/types/schema/*,src/**/*/*.stories.ts\"",
    "build:commonjs": "BABEL_ENV=cjs babel ./src --extensions \".ts,.js,.tsx\" -d lib --copy-files --ignore \"src/**/*.stories.tsx,src/proto/*,src/types/schema/*,src/**/*/*.stories.ts\"",
    "build:storybook": "mkdir -p public && build-storybook -o docs -s ./public",
    "build-protos": "bash ./scripts/build-protos.sh",
    "schema-to-ts": "json2ts -i third_party/schema -o src/types/schema/",
    "watch": "chokidar \"src/**/*.ts\" \"src/**/*.tsx\" -c \"npm run build:commonjs && npm run build:esm && npm run tsc:esm && npm run tsc:commonjs && npm run lint\"",
    "tsc:esm": "tsc --module es2015 --outDir esm",
    "tsc:commonjs": "tsc --module commonjs --outDir lib",
    "docs": "npm run clean:docs && npm run build:storybook && gh-pages -d docs",
    "lint": "eslint 'src/**/*.ts' 'src/**/*.tsx'; exit 0",
    "lint:watch": "chokidar \"src/**/*.ts\" \"src/**/*.tsx\" \".eslintrc\" -c \"npm run lint\"",
    "prepare": "npm run build",
    "preversion": "npm run docs",
    "postbuild": "node ./checkCircularDeps.js",
    "postversion": "git push origin HEAD --follow-tags",
    "start": "concurrently -r \"npm run storybook\" \"npm run lint:watch\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FigureTechnologies/frontlib-accessibility.git"
  },
  "keywords": [
    "react",
    "presentation"
  ],
  "author": "Figure",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/FigureTechnologies/frontlib-accessibility/issues"
  },
  "homepage": "https://github.com/FigureTechnologies/frontlib-accessibility#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "7.12.7",
    "@babel/plugin-transform-object-assign": "^7.12.1",
    "@babel/plugin-transform-react-constant-elements": "7.12.1",
    "@babel/plugin-transform-runtime": "7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@storybook/addon-a11y": "6.1.17",
    "@storybook/addon-actions": "6.1.17",
    "@storybook/addon-essentials": "6.1.17",
    "@storybook/addon-links": "6.1.17",
    "@storybook/react": "6.1.17",
    "@types/create-hash": "1.2.2",
    "@types/google-protobuf": "3.7.4",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.7",
    "@types/secp256k1": "4.0.1",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "babel-loader": "8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "chalk": "4.1.0",
    "chokidar-cli": "2.1.0",
    "concurrently": "5.3.0",
    "dedent": "0.7.0",
    "eslint": "7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "focus-visible": "5.2.0",
    "grpc_tools_node_protoc_ts": "5.1.3",
    "json-schema-to-typescript": "10.1.4",
    "madge": "3.12.0",
    "prettier": "2.2.1",
    "protoc-gen-grpc-web": "1.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "5.2.0",
    "storybook-addon-designs": "5.4.3",
    "styled-components": "^5.2.1",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@tendermint/belt": "0.3.0",
    "@tendermint/sig": "0.6.0",
    "@types/create-hash": "1.2.2",
    "@types/google-protobuf": "3.7.4",
    "@types/secp256k1": "4.0.1",
    "google-protobuf": "3.15.8",
    "grpc-tools": "1.11.1",
    "grpc-web": "1.2.1",
    "secp256k1": "4.0.2"
  }
}
